.thumb
.syntax unified

.include "gpio_constants.s"     // Register-adresser og konstanter for GPIO

.text
	.global Start
	
Start:


	LDR R5, =SysTick_CTRL_CLKSOURCE_Msk
	ORR R5, R5, =SysTick_CTRL_TICKINT_Msk
	ORR R5, R5, =SysTick_CTRL_ENABLE_Msk //R5 is now a mask for our SysTick_CTRL
	LDR R6, =SYSTICK_BASE
	STR R5, [R6]



	// LED TEST

	LDR R0, =GPIO_BASE
	LDR R1, =PORT_SIZE
	LDR R2, =LED_PORT
	LDR R3, =LED_PIN
	MUL R1, R1, R2 //Gives offset to Port E, R1 is address for PORT E
	ADD R0, R0, R1 //Adds R1 offset to R0
	ADD R0, R0, R3 //Adds R3 offset to R0
	MOV R4, #1
	STR R4, [R0]






NOP // Behold denne p√• bunnen av fila

